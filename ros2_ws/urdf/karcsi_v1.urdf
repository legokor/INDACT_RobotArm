<?xml version="1.0"?>

<robot name="karcsi_v1">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.25 0.2 0.2"/>
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
        </visual>
    </link>
    <link name="1_link">
        <visual>
            <geometry>
                <box size="0.04 0.04 0.6"/>
            </geometry>
            <origin xyz="0 0 0.3" rpy="0 0 0"/>
        </visual>
    </link>
    <link name="2_link">
        <visual>
            <geometry>
                <box size="0.08 0.02 0.08"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
    </link>
    <link name="3_link">
        <visual>
            <geometry>
                <box size="0.04 0.04 0.6"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 1.5708 0"/>
        </visual>
    </link>
    <link name="ee_link">
        <visual>
            <geometry>
                <box size="0.08 0.06 0.02"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
    </link>

    <joint name="base_1_joint" type="revolute">
        <parent link="base_link"/>
        <child link="1_link"/>
        <origin xyz="0 0 0.2" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3" upper="3" velocity="100" effort="100"/>
    </joint>
    <joint name="1_2_joint" type="prismatic">
        <parent link="1_link"/>
        <child link="2_link"/>
        <origin xyz="0 -0.03 0.6" rpy="0 0 0"/>
        <axis xyz="0 0 -1"/>
        <limit lower="0" upper="0.6" velocity="100" effort="100"/>
    </joint>
    <joint name="2_3_joint" type="prismatic">
        <parent link="2_link"/>
        <child link="3_link"/>
        <origin xyz="-0.2 -0.01 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0.4" velocity="100" effort="100"/>
    </joint>
    <joint name="3_ee_joint" type="fixed">
        <parent link="3_link"/>
        <child link="ee_link"/>
        <origin xyz="0.3 -0.03 0" rpy="1.5708 0 0"/>
    </joint>
</robot>